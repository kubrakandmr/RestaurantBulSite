<head>

    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="author" content="Surjith S M">
    <meta name="description" content="#">
    <meta name="keywords" content="#">

    <script src="../../cdn-cgi/apps/head/-mEFVS8y7qx5pVzWHQTCQu5gnVM.js"></script>
    <link rel="shortcut icon" href="#">

    <title>Listed</title>

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/bootstrap.min.css">

    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900" rel="stylesheet">

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/themify-icons.css">

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/font-awesome.min.css">

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/simple-line-icons.css">

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/swiper.min.css">

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/magnific-popup.css">

    <link href="~/Content/tempp/Sablon/listed/css/set1.css" rel="stylesheet">

    <link rel="stylesheet" href="~/Content/tempp/Sablon/listed/css/style.css">
</head>
<body>
    <section class="gray-dark booking-details_wrap">
        <div class="container">

            <div class="row">
                <div class="col-md-12 responsive-wrap">
                    <div class="booking-checkbox_wrap">
                        <h5>@Model.PlaceName</h5>
                        <ul class="card-rating">


                            <li> @Model.Point</li>
                            <li><i class="fa fa-circle" aria-hidden="true"></i></li>
                            <li>@Model.AvgPrice tl</li>

                        </ul>

                    </div>
                </div>
            </div>
        </div>
    </section>

    <section class="gray-dark booking-details_wrap">
        <div class="container">
            <div class="row">
                <div class="col-md-12 responsive-wrap">
                   
                        <img src="/@Model.MenuPic"  />
                  
                </div>
            </div>
        </div>
    </section>




    <section class="gray-dark booking-details_wrap">
        <div class="container">
            <div class="row">


                <div class="col-md-8 responsive-wrap">
                    <div class="booking-checkbox_wrap">
                        @{
                            Html.RenderAction("SecilenEkler", "Places", new { id = Model.PlaceID });
                        }
                    </div>

                        <div class="booking-checkbox_wrap my-4">
                            <h4>Yorumlar</h4>
                            <hr>
                            @{
                                Html.RenderAction("CommentList", "Places", new { id = Model.PlaceID });
                            }
                            <hr>

                        </div>
                    </div>
                    <div class="col-md-4 responsive-wrap">
                        <div class="contact-info">
                            @*<img src="images/map.jpg" class="img-fluid" alt="#">*@
                            <div class="address">
                                <span class="icon-location-pin"></span>
                                <p>

                                    <br> @Model.Address
                                    <br> @Model.County
                                    <br> @Model.City
                                </p>
                            </div>
                            <div class="address">
                                <span class="icon-screen-smartphone"></span>
                                <p>@Model.Phone</p>
                            </div>

                            <div class="address">
                                <span class="icon-clock"></span>
                                <p>@Model.OpenTime - @Model.CloseTime</p>
                                <a href="#" class="featured-open">Açık</a>
                            </div>
                            <a href="#" class="btn btn-outline-danger btn-contact">Mesaj Gönder</a>
                        </div>

                    </div>
                </div>
        </div>

    </section>







    <script src="~/Content/tempp/Sablon/listed/js/popper.min.js"></script>
    <script src="~/Content/tempp/Sablon/listed/js/jquery-3.2.1.min.js"></script>
    <script src="~/Content/tempp/Sablon/listed/js/bootstrap.min.js"></script>



    <script src="js/jquery.magnific-popup.js"></script>

    <script src="js/swiper.min.js"></script>
    <script>
        var swiper = new Swiper('.swiper-container', {
            slidesPerView: 3,
            slidesPerGroup: 3,
            loop: true,
            loopFillGroupWithBlank: true,
            pagination: {
                el: '.swiper-pagination',
                clickable: true,
            },
            navigation: {
                nextEl: '.swiper-button-next',
                prevEl: '.swiper-button-prev',
            },
        });
    </script>
    <script>
        if ($('.image-link').length) {
            $('.image-link').magnificPopup({
                type: 'image',
                gallery: {
                    enabled: true
                }
            });
        }
        if ($('.image-link2').length) {
            $('.image-link2').magnificPopup({
                type: 'image',
                gallery: {
                    enabled: true
                }
            });
        }
    </script>
</body>

