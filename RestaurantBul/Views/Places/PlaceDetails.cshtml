<body>
   
        <div>

            <div class="swiper-container">
                @*<div class="swiper-wrapper">*@
                    @*<div class="swiper-slide">*@
                        <a href="#"><img class="card-img-top" src="@Model.MenuPic" alt=""></a>
                        @*<a href="images/feat_slide1.jpg" class="grid image-link">

            <img src=@Model.MenuPic class="img-fluid">
        </a>*@
                    </div>
                  
                   
                    
                  
                  
              

                <div class="swiper-pagination swiper-pagination-white"></div>

                <div class="swiper-button-next swiper-button-white"></div>
                <div class="swiper-button-prev swiper-button-white"></div>
            @*</div>*@
        </div>


        <section class="reserve-block">
            <div class="container">
                <div class="row">
                    <div class="col-md-6">
                        <h5>@Model.PlaceName</h5>
                        <p>
                            <span>@Model.AvgPrice</span>$$
                        </p>

                    </div>
                    <div class="col-md-6">
                        <div class="reserve-seat-block">
                            <div class="reserve-rating mx-0 mx-md-3">
                                <span>@Model.Point</span>
                            </div>

                        </div>
                    </div>
                </div>
            </div>
        </section>


        <section class="gray-dark booking-details_wrap">
            <div class="container">
                <div class="row">


                    <div class="col-md-8 responsive-wrap">
                        <div class="booking-checkbox_wrap">
                          
                            <div class="row">
                                <div class="col-md-6 col-lg-4">
                                    <label class="custom-checkbox">
                                        <span class="ti-check-box"></span>
                                        <span class="custom-control-description">Bike Parking</span>
                                    </label>
                                </div>
                                <div class="col-md-6 col-lg-4">
                                    <label class="custom-checkbox">
                                        <span class="ti-check-box"></span>
                                        <span class="custom-control-description">Wireless Internet </span>
                                    </label>
                                </div>
                                <div class="col-md-6 col-lg-4">
                                    <label class="custom-checkbox">
                                        <span class="ti-check-box"></span>
                                        <span class="custom-control-description">Smoking Allowed </span>
                                    </label>
                                </div>
                                <div class="col-md-6 col-lg-4">
                                    <label class="custom-checkbox">
                                        <span class="ti-check-box"></span>
                                        <span class="custom-control-description">Street Parking</span>
                                    </label>
                                </div>
                                <div class="col-md-6 col-lg-4">
                                    <label class="custom-checkbox">
                                        <span class="ti-check-box"></span>
                                        <span class="custom-control-description">Special</span>
                                    </label>
                                </div>
                                <div class="col-md-6 col-lg-4">
                                    <label class="custom-checkbox">
                                        <span class="ti-check-box"></span>
                                        <span class="custom-control-description">Accepts Credit cards</span>
                                    </label>
                                </div>
                            </div>
                        </div>


                        <div class="booking-checkbox_wrap my-4">
                            <h4>Yorumlar</h4>
                            <hr>
                            @{ 
                                Html.RenderAction("CommentList", "Places", new { id = Model.PlaceID});
                            }
                            @*@Html.Action("CommentList")*@


                            <div class="customer-review_wrap">
                                <div class="customer-img">
                                    <img src="images/customer-img1.jpg" class="img-fluid" alt="#">
                                    <p>@Model.Name</p>
                                    <span>35 Reviews</span>
                                </div>
                                <div class="customer-content-wrap">
                                    <div class="customer-content">
                                        <div class="customer-review">

                                            <span></span>
                                            <span></span>
                                            <span></span>
                                            <span></span>
                                            <span class="round-icon-blank"></span>

                                        </div>
                                        <div class="customer-rating">8.0</div>
                                    </div>
                                    <p class="customer-text">
                                        @Model.Content
                                    </p>
                                    <p class="customer-text">
                                        @Model.Content
                                    </p>
                                    <ul>
                                        <li>
                                            @*<img src="@Model.CommentPic" class="img-fluid" alt="#">*@
                                            <img width="75" src="@Model.CommentPic" />
                                        </li>
                                       
                                    </ul>


                                </div>
                            </div>

                            <hr>

                        </div>
                    </div>
                    <div class="col-md-4 responsive-wrap">
                        <div class="contact-info">
                            @*<img src="images/map.jpg" class="img-fluid" alt="#">*@
                            <div class="address">
                                <span class="icon-location-pin"></span>
                                <p>

                                    <br> @Model.Address
                                    <br> @Model.County
                                    <br> @Model.City
                                </p>
                            </div>
                            <div class="address">
                                <span class="icon-screen-smartphone"></span>
                                <p>@Model.Phone</p>
                            </div>
                            
                            <div class="address">
                                <span class="icon-clock"></span>
                                <p>@Model.OpenTime - @Model.CloseTime</p>
                                <a href="#" class="featured-open">Açık</a>
                            </div>
                            <a href="#" class="btn btn-outline-danger btn-contact">Mesaj Gönder</a>
                        </div>

                    </div>
                </div>
            </div>
           
        </section>







    <script src="~/Content/tempp/Sablon/listed/js/popper.min.js"></script>
    <script src="~/Content/tempp/Sablon/listed/js/jquery-3.2.1.min.js"></script>
    <script src="~/Content/tempp/Sablon/listed/js/bootstrap.min.js"></script>



    <script src="js/jquery.magnific-popup.js"></script>

        <script src="js/swiper.min.js"></script>
    <script>
            var swiper = new Swiper('.swiper-container', {
                slidesPerView: 3,
                slidesPerGroup: 3,
                loop: true,
                loopFillGroupWithBlank: true,
                pagination: {
                    el: '.swiper-pagination',
                    clickable: true,
                },
                navigation: {
                    nextEl: '.swiper-button-next',
                    prevEl: '.swiper-button-prev',
                },
            });
        </script>
        <script>
            if ($('.image-link').length) {
                $('.image-link').magnificPopup({
                    type: 'image',
                    gallery: {
                        enabled: true
                    }
                });
            }
            if ($('.image-link2').length) {
                $('.image-link2').magnificPopup({
                    type: 'image',
                    gallery: {
                        enabled: true
                    }
                });
            }
        </script>
</body>

